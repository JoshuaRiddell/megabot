<?xml version="1.0"?>

<robot name="tennis_ball">

  <!-- root link, on the ground just below the model origin -->
  <link name="ball">

    <inertial>
      <mass value="0.058" />
      <origin xyz="0 0 0" />
      <inertia ixx="0.000032" ixy="0.0" ixz="0.0"
        iyy="0.000032" iyz="0.0"
        izz="0.000032" />
    </inertial>

   <visual>
      <origin xyz="0 0 0" rpy="0 0 0" />
      <geometry>
        <sphere radius="0.066"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0 0 0" />
      <geometry>
        <sphere radius="0.066"/>
      </geometry>
    </collision>

  </link>

  <gazebo reference="ball">
    <mu1>10</mu1>
    <mu2>10</mu2>
    <material>Gazebo/Green</material>
    <dampingFactor>0.0001</dampingFactor>
  </gazebo>

</robot>

